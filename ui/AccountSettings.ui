/* --- Styl widżetu ustawień konta --- */
QWidget#AccountSettingsWidget {
    background-color: #2f3136;
    color: #e8e9ea;
    font-family: "Segoe UI", Roboto, Cantarell, "Helvetica Neue", sans-serif;
    font-size: 14px;
}

/* --- Styl centralnego kontenera --- */
QWidget#contentWidget {
    background-color: #36393f;
    border-radius: 8px;
    padding: 25px;
}

/* --- Avatar --- */
QLabel#avatarPlaceholder {
    background-color: #4f545c; /* Ciemniejszy szary */
    border: 2px solid #5865F2; /* Ramka w kolorze akcentu */
    border-radius: 50px; /* Połowa szerokości/wysokości (100/2) dla koła */
    min-width: 100px;
    max-width: 100px;
    min-height: 100px;
    max-height: 100px;
    qproperty-alignment: 'AlignCenter'; /* Gdybyś chciał dodać inicjały */
    color: #dcddde;
    font-size: 40px;
    font-weight: 600;
    margin-bottom: 15px; /* Odstęp pod avatarem */
}


/* --- Ogólny styl etykiet --- */
QLabel {
    color: #b9bbbe;
    background-color: transparent;
    padding: 0px;
    margin: 0px;
    font-size: 14px;
    font-weight: 500;
}

/* --- Styl tytułu --- */
QLabel#titleLabel {
    color: #ffffff;
    font-size: 26px;
    font-weight: 600;
    padding-top: 5px;
    padding-bottom: 30px; /* Zwiększony odstęp pod tytułem */
}

/* --- Styl pól edycji --- */
QLineEdit {
    background-color: #40444b;
    color: #e8e9ea;
    border: 1px solid #4f545c; /* Lekko widoczniejsza ramka */
    border-radius: 5px;
    font-size: 14px;
    padding: 10px 12px; /* Zwiększony padding pionowy */
    min-height: 30px; /* Zaktualizowana minimalna wysokość */
    selection-background-color: #5865F2;
    selection-color: #ffffff;
}
QLineEdit:focus {
    border: 1px solid #5865F2; /* Ramka przy fokusie */
    background-color: #444950;
}
QLineEdit::placeholder-text {
  color: #72767d;
}
QLineEdit:read-only {
    background-color: #3a3d42;
    color: #a0a3a7;
    border: 1px solid #2a2c30;
}


/* --- Styl przycisku zapisu --- */
QPushButton#saveChanges {
    background-color: #5865F2;
    color: white;
    font-size: 14px;
    font-weight: 600;
    padding: 10px 28px; /* Zwiększony padding */
    border-radius: 5px;
    border: none;
    min-height: 34px; /* Zaktualizowana minimalna wysokość */
    outline: none;
    margin-top: 20px;
}
QPushButton#saveChanges:hover {
    background-color: #4e5dcf;
}
QPushButton#saveChanges:pressed {
    background-color: #404ab8;
}
QPushButton#saveChanges:disabled {
    background-color: #40444b;
    color: #72767d;
    opacity: 0.7;
}

/* --- Styl etykiety statusu/błędu --- */
QLabel#statusLabel {
    color: #43b581;
    font-size: 13px;
    font-weight: 500;
    min-height: 22px;
    padding: 5px 0;
}
