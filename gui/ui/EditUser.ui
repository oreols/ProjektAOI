/* Zastosowany do głównego QDialog#Dialog */
QDialog#Dialog {
    background-color: #2f3136;
    color: #e8e9ea;
    font-family: "Segoe UI", Roboto, Cantarell, "Helvetica Neue", sans-serif;
    font-size: 14px;
}

/* --- Styl centralnego kontenera --- */
QWidget#contentWidget { /* Ten widget będzie zachowany dla struktury */
    background-color: #36393f;
    border-radius: 8px;
    padding: 25px;
}

/* --- Avatar --- */
QLabel#avatarPlaceholder { /* Zachowany dla wyglądu */
    background-color: #4f545c; /* Ciemniejszy szary */
    border: 2px solid #5865F2; /* Ramka w kolorze akcentu */
    border-radius: 50px; /* Połowa szerokości/wysokości (100/2) dla koła */
    min-width: 100px;
    max-width: 100px;
    min-height: 100px;
    max-height: 100px;
    qproperty-alignment: 'AlignCenter';
    color: #dcddde;
    font-size: 40px;
    font-weight: 600;
    margin-bottom: 15px; /* Odstęp pod avatarem */
}


/* --- Ogólny styl etykiet --- */
QLabel {
    color: #b9bbbe;
    background-color: transparent;
    padding: 0px;
    margin: 0px;
    font-size: 14px;
    font-weight: 500;
}

/* --- Styl tytułu (Używamy QLabel#label z oryginalnego Dialog) --- */
QLabel#label { /* Celuje w tytuł 'Edytuj użytkownika' */
    color: #ffffff;
    font-size: 26px;
    font-weight: 600;
    padding-top: 5px;
    padding-bottom: 30px; /* Zwiększony odstęp pod tytułem */
    /* Alignment ustawiony w layoucie */
}

/* --- Styl pól edycji (QLineEdit) --- */
QLineEdit {
    background-color: #40444b;
    color: #e8e9ea;
    border: 1px solid #4f545c;
    border-radius: 5px;
    font-size: 14px;
    padding: 10px 12px;
    min-height: 30px;
    selection-background-color: #5865F2;
    selection-color: #ffffff;
}
QLineEdit:focus {
    border: 1px solid #5865F2;
    background-color: #444950;
}
QLineEdit::placeholder-text {
  color: #72767d;
}
/* QLineEdit:read-only niepotrzebny w tym UI */


/* --- Styl dla QComboBox (bazujący na QLineEdit) --- */
QComboBox {
    background-color: #40444b;
    color: #e8e9ea;
    border: 1px solid #4f545c;
    border-radius: 5px;
    font-size: 14px;
    padding: 10px 12px;
    min-height: 30px;
    selection-background-color: #5865F2; /* Kolor zaznaczenia w liście */
    selection-color: #ffffff;
}
QComboBox:focus {
    border: 1px solid #5865F2;
    background-color: #444950;
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 25px;
    border-left-width: 1px;
    border-left-color: #4f545c;
    border-left-style: solid;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    background-color: transparent;
}
QComboBox::down-arrow {
    image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAhklEQVQ4T+3SMQrCUAzG8f+7hQhBCNFbkBCsEFKqHUXUFuIJKPQkZC4nkVx4CPchLQa33txh7m3uwzf09/s9rAVhZu/yv3dAHlgvR6gAVf4FAClAp7gBh6AL3ICVYPMK6gHW4NO+gYqwFfwB2sEHsAJOwBMswAWEzgGc1S18AP79AhIh5hJIAAAAAElFTkSuQmCC); /* Biała strzałka */
    width: 12px;
    height: 12px;
}
/* Styl listy rozwijanej */
QComboBox QAbstractItemView {
    background-color: #36393f; /* Tło listy */
    border: 1px solid #4f545c; /* Ramka listy */
    color: #e8e9ea; /* Kolor tekstu w liście */
    selection-background-color: #5865F2; /* Kolor zaznaczenia */
    selection-color: #ffffff;
    padding: 4px;
    outline: 0px; /* Usunięcie ramki focusa */
}


/* --- Styl dla QCheckBox --- */
QCheckBox {
    color: #b9bbbe;
    spacing: 8px;
    font-size: 14px;
    font-weight: 500;
 }
QCheckBox::indicator {
    width: 20px;
    height: 20px;
    background-color: #40444b;
    border: 1px solid #4f545c;
    border-radius: 3px;
}
QCheckBox::indicator:hover {
    border: 1px solid #5c616a;
}
QCheckBox::indicator:checked {
    background-color: #5865F2;
    border: 1px solid #4e5dcf;
    image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAgUlEQVQ4T2NkoDJgpLJ5DKMvQ3z58uV/BkT4/+9/huNAPH/+/H9mBII/gHgDEsB4/v37H4G4EEYDQNwMxAGkG0C8COpXgPg/UJsBQI0LIG4A0g0gHgOQNwApDyD+BxCvAakNASkOpBsgPj0gvgNSAABpLUz0725+AAAAAElFTkSuQmCC); /* Biały ptaszek */
}
QCheckBox::indicator:disabled {
    background-color: #3a3d42;
    border: 1px solid #2a2c30;
}


/* --- Styl przycisku (Używamy QPushButton#addAccountButton z oryginalnego Dialog) --- */
QPushButton#addAccountButton { /* Celuje w przycisk 'Dodaj' */
    background-color: #5865F2;
    color: white;
    font-size: 14px;
    font-weight: 600;
    padding: 10px 28px;
    border-radius: 5px;
    border: none;
    min-height: 34px;
    outline: none;
    /* margin-top ustawiony w layoucie */
}
QPushButton#addAccountButton:hover {
    background-color: #4e5dcf;
}
QPushButton#addAccountButton:pressed {
    background-color: #404ab8;
}
QPushButton#addAccountButton:disabled {
    background-color: #40444b;
    color: #72767d;
    opacity: 0.7;
}
/* Styl QPushButton#saveChanges ignorowany */
