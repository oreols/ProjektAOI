/* --- Styl Zarządzania Użytkownikami --- */
QDialog#AccountDialog {
    background-color: #2f3136; /* Główne tło */
    color: #e8e9ea;
    font-family: &quot;Segoe UI&quot;, Roboto, Cantarell, &quot;Helvetica Neue&quot;, sans-serif;
    font-size: 14px;
}

/* --- Górny i Dolny Pasek --- */
QFrame#topToolbar, QFrame#bottomBar {
    background-color: #2f3136; /* Tło pasków */
    border: none; /* Bez zaokrągleń i ramek dla pasków */
    padding: 0;
    min-height: 50px;
    max-height: 50px;
}
QFrame#topToolbar { border-bottom: 1px solid #202225; }
QFrame#bottomBar { border-top: 1px solid #202225; }

/* Layouty wewnątrz pasków */
#topToolbar QHBoxLayout, #bottomBar QHBoxLayout {
    spacing: 15px;
    margin: 0px 20px; /* Marginesy L/R dla zawartości */
}
QLabel#titleLabel {
    color: #ffffff;
    font-size: 18px;
    font-weight: 600;
    background-color: transparent;
}

/* --- Tabela --- */
QTableWidget#tableWidget {
    background-color: #36393f; /* Tło jak paski */
    border: 1px solid #202225; /* Subtelna ramka */
    border-radius: 8px; /* --- ZAOKRĄGLENIE ROGÓW --- */
    gridline-color: #40444b;
    color: #e8e9ea;
    font-size: 14px;
    selection-background-color: #5865F2;
    selection-color: #ffffff;
    alternate-background-color: #40444b;
    outline: none;
}
/* Wygląd komórek */
QTableWidget#tableWidget::item {
    padding: 12px 15px; /* Zwiększony padding dla lepszej czytelności */
    border: none;
    border-bottom: 1px solid #40444b; /* Linia oddzielająca wiersze */
}

/* --- Nagłówki Tabeli --- */
QHeaderView {
    border: none;
    /* Tło dziedziczone z tableWidget lub można ustawić np. #36393f */
    background-color: transparent; /* Przezroczyste, aby pokazać tło tabeli */
}
QHeaderView::section {
    background-color: transparent; /* Przezroczyste */
    color: #ffffff;
    padding: 14px 15px; /* Wysokość nagłówka */
    border: none;
    font-size: 14px;
    font-weight: 600;
    text-align: left;
}
QHeaderView::section:horizontal {
    border-bottom: 1px solid #202225; /* Ciemniejsza linia pod nagłówkiem */
    border-right: 1px solid #202225; /* Ciemniejsza linia między */
}
QHeaderView::section:horizontal:last { border-right: none; }

/* Narożnik tabeli - ważne, gdyby nagłówki miały inne tło */
QTableWidget QTableCornerButton::section {
    background-color: #36393f; /* Jak tło tabeli */
    border: none;
    border-right: 1px solid #202225;
    border-bottom: 1px solid #202225;
    border-top-left-radius: 8px; /* Spróbuj zaokrąglić róg */
}


/* --- Przyciski --- */
QPushButton {
    background-color: #5865F2;
    color: white;
    font-size: 14px;
    font-weight: 600;
    padding: 8px 20px;
    border-radius: 5px;
    border: none;
    min-height: 30px;
    outline: none;
}
QPushButton:hover { background-color: #4e5dcf; }
QPushButton:pressed { background-color: #404ab8; }
QPushButton:disabled { background-color: #40444b; color: #72767d; opacity: 0.6; }

/* Paski przewijania */
QScrollBar:vertical { border: 1px solid #202225; background: #36393f; width: 10px; margin: 1px 0 1px 0; border-radius: 5px;}
QScrollBar::handle:vertical { background: #40444b; min-height: 25px; border-radius: 5px; }
QScrollBar::handle:vertical:hover { background: #444950; }
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical { height: 0px; border: none; background: none;}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical { background: none; }

QScrollBar:horizontal { border: 1px solid #202225; background: #36393f; height: 10px; margin: 0 1px 0 1px; border-radius: 5px;}
QScrollBar::handle:horizontal { background: #40444b; min-width: 25px; border-radius: 5px; }
QScrollBar::handle:horizontal:hover { background: #444950; }
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal { width: 0px; border: none; background: none;}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal { background: none; }
